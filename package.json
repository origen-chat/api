{
  "name": "@origen/api",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development nodemon --inspect=0.0.0.0:9229 --nolazy --config nodemon.json src/index.ts",
    "start:dev:no-debug": "cross-env NODE_ENV=development nodemon --config nodemon.json src/index.ts",
    "build:prod": "NODE_ENV=production babel --extensions \".ts\" --out-dir dist src",
    "download-graphql-schema": "apollo schema:download schema.json",
    "predownload-graphql-schema": "yarn clean-graphql-schema",
    "migrate": "knex --knexfile=knexfile.ts migrate:latest",
    "make-migration": "knex --knexfile=knexfile.ts migrate:make",
    "rollback-migration": "knex --knexfile=knexfile.ts migrate:rollback",
    "make-keys": "mkcert localhost api.local.origen.chat 127.0.0.1 ::1 0.0.0.0",
    "premake-keys": "yarn clean-keys",
    "postmake-keys": "mkdirp keys && mv localhost+4.pem keys/shared.crt && mv localhost+4-key.pem keys/shared.key",
    "test": "cross-env NODE_ENV=test jest",
    "pretest": "cross-env NODE_ENV=test yarn migrate",
    "test:ci": "yarn test --coverage",
    "test:watch": "jest --onlyChanged --watch",
    "prebuild": "yarn typecheck && yarn clean-dist",
    "typecheck": "tsc",
    "clean-dist": "rimraf dist",
    "clean-keys": "rimraf keys/shared.key keys/shared.crt",
    "clean-graphql-schema": "rimraf schema.json",
    "logs": "docker-compose logs --follow --tail=10",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,ts}\""
  },
  "dependencies": {
    "@google-cloud/storage": "2.3.4",
    "@sentry/node": "4.5.3",
    "@strattadb/environment": "5.2.0",
    "ajv": "6.7.0",
    "apollo-server-express": "2.3.2",
    "bluebird": "3.5.3",
    "body-parser": "1.18.3",
    "bull": "3.6.0",
    "chalk": "2.4.2",
    "cors": "2.8.5",
    "dataloader": "1.4.0",
    "date-fns": "2.0.0-alpha.27",
    "express": "4.16.4",
    "graphql": "14.1.1",
    "graphql-iso-date": "3.6.1",
    "graphql-redis-subscriptions": "2.0.0",
    "graphql-subscriptions": "1.0.0",
    "graphql-type-json": "0.2.1",
    "ioredis": "4.5.1",
    "jsonwebtoken": "8.4.0",
    "knex": "0.16.3",
    "mailgun-js": "0.22.0",
    "ms": "2.1.1",
    "passport": "0.4.0",
    "passport-google-oauth20": "1.0.0",
    "passport-jwt": "4.0.0",
    "pg": "7.8.0",
    "ramda": "0.26.1",
    "sharp": "0.21.3",
    "stripe": "6.22.0",
    "uuid": "3.3.2",
    "web-push": "3.3.3",
    "winston": "3.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/node": "7.2.2",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-typescript": "7.1.0",
    "@babel/register": "7.0.0",
    "@commitlint/cli": "7.3.2",
    "@commitlint/config-conventional": "7.3.1",
    "@commitlint/prompt-cli": "7.3.1",
    "@commitlint/travis-cli": "7.3.2",
    "@types/bluebird": "3.5.25",
    "@types/bull": "3.5.5",
    "@types/dotenv": "6.1.0",
    "@types/graphql": "14.0.5",
    "@types/graphql-iso-date": "3.3.1",
    "@types/graphql-type-json": "0.1.3",
    "@types/ioredis": "4.0.6",
    "@types/jest": "23.3.13",
    "@types/json-schema": "7.0.1",
    "@types/jsonwebtoken": "8.3.0",
    "@types/mailgun-js": "0.16.3",
    "@types/ms": "0.7.30",
    "@types/node": "10.12.18",
    "@types/passport": "1.0.0",
    "@types/passport-google-oauth": "1.0.39",
    "@types/passport-jwt": "3.0.1",
