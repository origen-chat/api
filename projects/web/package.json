  "private": true,
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development webpack-serve --config webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "graphql:codegen:generate:watch": "yarn graphql:codegen:generate --watch",
    "graphql:codegen:generate": "apollo codegen:generate --addTypename --queries=src/**/*.tsx --target=typescript --schema=../api/schema.json __generatedTypes__",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "yarn test --onlyChanged --watch",
    "test:ci": "yarn test --coverage",
    "typecheck:ts": "yarn tsc"
  },
