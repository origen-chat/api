{
  "name": "loop-api",
  "version": "2.0.0",
  "private": true,
  "license": "MIT",
  "main": "dist/app.js",
  "scripts": {
    "start:dev": "NODE_ENV=development nodemon --inspect=0.0.0.0:9229 --nolazy --config nodemon.json src/index.ts",
    "build:prod": "NODE_ENV=production babel --extensions \".ts,.tsx\" --out-dir dist src",
    "download-graphql-schema": "apollo schema:download schema.json",
    "predownload-graphql-schema": "rimraf schema.json",
    "migrate": "knex migrate:latest",
    "make-migration": "knex migrate:make",
    "rollback-migration": "knex migrate:rollback",
    "test": "NODE_ENV=test jest",
    "pretest": "NODE_ENV=test yarn migrate",
    "test:ci": "yarn test --coverage",
    "test:watch": "jest --onlyChanged --watch",
    "prebuild": "yarn typecheck && yarn clean",
    "typecheck": "yarn typecheck:ts",
    "typecheck:ts": "tsc",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@sentry/node": "4.4.0",
    "@strattadb/environment": "5.1.0",
    "ajv": "6.5.5",
    "apollo-server-express": "2.2.4",
    "bluebird": "3.5.3",
    "body-parser": "1.18.3",
    "bull": "3.5.2",
    "chalk": "2.4.1",
    "cors": "2.8.5",
    "dataloader": "1.4.0",
    "date-fns": "1.29.0",
    "express": "4.16.4",
    "graphql": "14.0.2",
    "graphql-iso-date": "3.6.1",
    "graphql-redis-subscriptions": "2.0.0",
    "graphql-subscriptions": "1.0.0",
    "graphql-type-json": "0.2.1",
    "ioredis": "4.2.0",
    "jsonwebtoken": "8.4.0",
    "knex": "0.15.2",
    "ms": "2.1.1",
    "passport": "0.4.0",
    "passport-google-oauth20": "1.0.0",
    "passport-jwt": "4.0.0",
    "pg": "7.6.1",
    "ramda": "0.26.1",
    "stripe": "6.17.0",
    "uuid": "3.3.2",
    "web-push": "3.3.3",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.1.5",
    "@babel/core": "7.1.6",
    "@babel/node": "7.0.0",
    "@babel/plugin-proposal-optional-catch-binding": "7.0.0",
    "@babel/preset-env": "7.1.6",
    "@babel/preset-typescript": "7.1.0",
    "@babel/register": "7.0.0",
    "@types/bluebird": "3.5.24",
    "@types/bull": "3.4.1",
    "@types/graphql": "14.0.3",
    "@types/graphql-iso-date": "3.3.1",
    "@types/graphql-type-json": "0.1.3",
    "@types/ioredis": "4.0.4",
    "@types/jest": "23.3.9",
    "@types/json-schema": "7.0.1",
    "@types/jsonwebtoken": "8.3.0",
    "@types/knex": "0.15.1",
    "@types/ms": "0.7.30",
    "@types/node": "10.12.10",
    "@types/passport": "0.4.7",
    "@types/passport-google-oauth": "1.0.39",
    "@types/passport-jwt": "3.0.1",
    "@types/ramda": "0.26.0",
    "@types/stripe": "6.0.4",
    "@types/uuid": "3.4.4",
    "@types/web-push": "3.3.0",
    "apollo": "2.1.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-plugin-dynamic-import-node": "2.2.0",
    "jest": "23.6.0",
    "nodemon": "1.18.7",
    "rimraf": "2.6.2",
    "ts-node": "7.0.1",
    "typescript": "3.1.6"
  },
  "engines": {
    "node": ">= 10.6.0"
  }
}
