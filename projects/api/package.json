{
  "name": "loop_api",
  "version": "1.0.20",
  "private": true,
  "license": "MIT",
  "main": "dist/app.js",
  "scripts": {
    "start:prod": "node dist/index.js",
    "start:dev": "nodemon --exec babel-node --extensions \".ts,.tsx\" src/index.ts",
    "build": "babel --extensions \".ts,.tsx\" --out-dir dist src",
    "graphql:schema:download": "apollo schema:download schema.json",
    "pregraphql:schema:download": "rimraf schema.json",
    "test": "jest",
    "test:ci": "yarn test --coverage",
    "test:watch": "jest --onlyChanged --watch",
    "prebuild": "yarn typecheck:ts && yarn clean",
    "typecheck:ts": "tsc",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-rc.1",
    "@babel/core": "7.0.0-rc.1",
    "@babel/node": "7.0.0-rc.1",
    "@babel/plugin-proposal-optional-catch-binding": "7.0.0-rc.1",
    "@babel/preset-env": "7.0.0-rc.1",
    "@babel/preset-typescript": "7.0.0-rc.1",
    "@types/graphql": "0.13.4",
    "@types/jest": "23.3.1",
    "@types/knex": "0.14.21",
    "@types/node": "10.7.1",
    "apollo": "^1.7.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.4.2",
    "babel-plugin-dynamic-import-node": "2.0.0",
    "jest": "23.5.0",
    "nodemon": "1.18.3",
    "rimraf": "2.6.2",
    "ts-node": "7.0.1",
    "typescript": "3.0.1"
  },
  "dependencies": {
    "apollo-server-express": "2.0.4",
    "core-js": "2.5.7",
    "express": "4.16.3",
    "graphql": "0.13.2",
    "knex": "0.15.2",
    "pg": "7.4.3",
    "winston": "3.0.0"
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "engines": {
    "node": ">= 10.0.0"
  }
}
