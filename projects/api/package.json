{
  "name": "loop_api",
  "version": "1.0.51",
  "private": true,
  "license": "MIT",
  "main": "dist/app.js",
  "scripts": {
    "start:prod": "node dist/index.js",
    "start:dev": "nodemon --exec babel-node --extensions \".ts,.tsx\" src/index.ts",
    "build": "babel --extensions \".ts,.tsx\" --out-dir dist src",
    "graphql:schema:download": "apollo schema:download schema.json",
    "pregraphql:schema:download": "rimraf schema.json",
    "test": "jest",
    "test:ci": "yarn test --coverage",
    "test:watch": "jest --onlyChanged --watch",
    "prebuild": "yarn typecheck:ts && yarn clean",
    "typecheck:ts": "tsc",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@babel/node": "7.0.0",
    "@babel/plugin-proposal-optional-catch-binding": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-typescript": "7.0.0",
    "@types/graphql": "14.0.0",
    "@types/jest": "23.3.2",
    "@types/jsonwebtoken": "7.2.8",
    "@types/knex": "0.14.24",
    "@types/node": "10.9.4",
    "@types/passport": "0.4.6",
    "@types/passport-google-oauth": "1.0.39",
    "@types/passport-jwt": "3.0.1",
    "apollo": "1.9.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-plugin-dynamic-import-node": "2.1.0",
    "jest": "23.6.0",
    "nodemon": "1.18.4",
    "rimraf": "2.6.2",
    "ts-node": "7.0.1",
    "typescript": "3.0.3"
  },
  "dependencies": {
    "apollo-server-express": "2.0.6",
    "core-js": "2.5.7",
    "cors": "2.8.4",
    "express": "4.16.3",
    "graphql": "14.0.2",
    "jsonwebtoken": "8.3.0",
    "knex": "0.15.2",
    "passport": "0.4.0",
    "passport-google-oauth20": "1.0.0",
    "passport-jwt": "4.0.0",
    "pg": "7.4.3",
    "winston": "3.1.0"
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "engines": {
    "node": ">= 10.0.0"
  }
}
