{
  "name": "loop-api",
  "version": "2.0.0",
  "private": true,
  "license": "MIT",
  "main": "dist/app.js",
  "scripts": {
    "start:prod": "NODE_ENV=production node dist/index.js",
    "start:dev": "NODE_ENV=development nodemon  --exec babel-node --extensions \".ts,.tsx\" src/index.ts",
    "build": "NODE_ENV=production babel --extensions \".ts,.tsx\" --out-dir dist src",
    "download-graphql-schema": "apollo schema:download schema.json",
    "predownload-graphql-schema": "rimraf schema.json",
    "migrate": "knex migrate:latest",
    "make-migration": "knex migrate:make",
    "rollback-migration": "knex migrate:rollback",
    "test": "NODE_ENV=test jest",
    "pretest": "NODE_ENV=test yarn migrate",
    "test:ci": "yarn test --coverage",
    "test:watch": "jest --onlyChanged --watch",
    "prebuild": "yarn typecheck && yarn clean",
    "typecheck": "yarn typecheck:ts",
    "typecheck:ts": "tsc",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@sentry/node": "4.2.3",
    "@strattadb/environment": "5.0.0",
    "apollo-server-express": "2.1.0",
    "chalk": "2.4.1",
    "core-js": "3.0.0-beta.3",
    "cors": "2.8.4",
    "express": "4.16.4",
    "graphql": "14.0.2",
    "graphql-redis-subscriptions": "2.0.0",
    "graphql-subscriptions": "1.0.0",
    "ioredis": "4.2.0",
    "jsonwebtoken": "8.3.0",
    "knex": "0.15.2",
    "passport": "0.4.0",
    "passport-google-oauth20": "1.0.0",
    "passport-jwt": "4.0.0",
    "pg": "7.6.0",
    "ramda": "0.25.0",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/node": "7.0.0",
    "@babel/plugin-proposal-optional-catch-binding": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-typescript": "7.1.0",
    "@babel/register": "7.0.0",
    "@types/graphql": "14.0.3",
    "@types/ioredis": "4.0.3",
    "@types/jest": "23.3.8",
    "@types/jsonwebtoken": "7.2.8",
    "@types/knex": "0.14.26",
    "@types/node": "10.12.0",
    "@types/passport": "0.4.6",
    "@types/passport-google-oauth": "1.0.39",
    "@types/passport-jwt": "3.0.1",
    "@types/ramda": "0.25.40",
    "apollo": "1.9.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-plugin-dynamic-import-node": "2.2.0",
    "jest": "23.6.0",
    "nodemon": "1.18.5",
    "rimraf": "2.6.2",
    "ts-node": "7.0.1",
    "typescript": "3.1.3"
  },
  "engines": {
    "node": ">= 10.0.0"
  }
}
